# Database
DATABASE_URL="postgresql://username:password@localhost:5432/hqmx"

# Redis
REDIS_URL="redis://localhost:6379"

# Cloudflare R2 Storage
R2_ACCOUNT_ID="your_account_id"
R2_ACCESS_KEY_ID="your_access_key_id"
R2_SECRET_ACCESS_KEY="your_secret_access_key"
R2_BUCKET_NAME="your_bucket_name"
R2_ENDPOINT="https://your_account_id.r2.cloudflarestorage.com"

# YouTube Data API (선택사항 - 분석용)
YOUTUBE_API_KEY="your_youtube_api_key"

# Residential Proxies (선택사항 - 봇 차단 방지용)
RESIDENTIAL_PROXIES="http://proxy1:port,http://proxy2:port"

# Instagram Credentials (선택사항 - 인스타그램 분석용)
INSTA_USER="your_instagram_username"
INSTA_PASS="your_instagram_password"

# Server Configuration
PORT=5001
NODE_ENV=production
TEMP_DIR="/tmp/hqmx"